
(defrule
	(goal current-phase 5)
	(and
		(unit-type-count-total polydynastus g:< gl-five-percent)
		(unit-type-count-total elite-polydynastus g:< gl-five-percent)
	)
	(or
		(can-train polydynastus)
		(can-train elite-polydynastus)
	)
=>
	(train polydynastus)
	(train elite-polydynastus)
)