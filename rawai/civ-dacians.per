
(defrule
	(goal current-phase 5)
	(and
		(unit-type-count-total falx-warrior g:< gl-five-percent)
		(unit-type-count-total elite-falx-warrior g:< gl-five-percent)
	)
	(or
		(can-train falx-warrior)
		(can-train elite-falx-warrior)
	)
=>
	(train falx-warrior)
	(train elite-falx-warrior)
)