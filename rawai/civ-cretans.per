
(defrule
	(goal current-phase 5)
	(and
		(unit-type-count-total cretan-archer g:< gl-five-percent)
		(unit-type-count-total elite-cretan-archer g:< gl-five-percent)
	)
	(or
		(can-train cretan-archer)
		(can-train elite-cretan-archer)
	)
=>
	(train cretan-archer)
	(train elite-cretan-archer)
)