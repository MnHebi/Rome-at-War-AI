
(defrule
	(goal current-phase 5)
	(and
		(unit-type-count-total immortal g:< desired-number-uniqueunitone)
		(unit-type-count-total elite-immortal g:< desired-number-uniqueunitone)
	)
	(or
		(can-train immortal)
		(can-train elite-immortal)
	)
=>
	(train immortal)
	(train elite-immortal)
)