
(defrule
	(goal current-phase 5)
	(and
		(unit-type-count-total medjay-warrior g:< gl-five-percent)
		(unit-type-count-total elite-medjay-warrior g:< gl-five-percent)
	)
	(or
		(can-train medjay-warrior)
		(can-train elite-medjay-warrior)
	)
=>
	(train medjay-warrior)
	(train elite-medjay-warrior)
)