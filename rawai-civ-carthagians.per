
(defrule
	(goal current-phase 5)
	(and
		(unit-type-count-total war-elephant g:< gl-five-percent)
		(unit-type-count-total elite-war-elephant g:< gl-five-percent)
	)
	(or
		(can-train war-elephant)
		(can-train elite-war-elephant)
	)
=>
	(train war-elephant)
	(train elite-war-elephant)
)