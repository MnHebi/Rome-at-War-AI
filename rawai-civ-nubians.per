
(defrule
	(goal current-phase 5)
	(and
		(unit-type-count-total archer-of-the-eyes g:< desired-number-uniqueunitone)
		(unit-type-count-total elite-archer-of-the-eyes g:< desired-number-uniqueunitone)
	)
	(or
		(can-train archer-of-the-eyes)
		(can-train elite-archer-of-the-eyes)
	)
=>
	(train archer-of-the-eyes)
	(train elite-archer-of-the-eyes)
)