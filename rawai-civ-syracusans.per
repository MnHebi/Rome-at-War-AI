
(defrule
	(goal current-phase 5)
	(and
		(unit-type-count-total oxybeles g:< desired-number-uniqueunitone)
		(unit-type-count-total elite-oxybeles g:< desired-number-uniqueunitone)
	)
	(or
		(can-train oxybeles)
		(can-train elite-oxybeles)
	)
=>
	(train oxybeles)
	(train elite-oxybeles)
)